{
  "url": "process.env.HEROKU_URL",
  "server": {
    "port": "process.env.PORT",
    "host": "0.0.0.0"
  },
  "database": {
    "client": "postgres",
    "connection": "process.env.DATABASE_URL",
    "debug": false
  },
  "mail": {
      "transport": "SMTP",
      "options": {
          "service": "Sendgrid",
          "auth": {
              "user": "process.env.SENDGRID_USERNAME",
              "pass": "process.env.SENDGRID_PASSWORD"
          }
      }
  },
  "logging": {
    "transports": [
      "file",
      "stdout"
    ]
  },
  "process": "local",
  "paths": {
    "contentPath": "content"
  }
}
