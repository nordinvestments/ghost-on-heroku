{
  "url": "url",
  "server": {
    "host": "0.0.0.0",
    "port": "server__port"
  },
  "database": {
    "client": "postgres",
    "connection": "database__connection"
  },
  "mail": {
    "transport": "SMTP",
    "options": {
      "service": "Sendgrid",
      "auth": {
        "user": "mail__options__auth__user",
        "pass": "mail__options__auth__pass"
      }
    }
  },
  "logging": {
    "transports": [
      "file",
      "stdout"
    ]
  },
  "process": "local",
  "paths": {
    "contentPath": "content"
  }
}
